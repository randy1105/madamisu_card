<div class="main">
  <div class="side-box">
   <ul class="lists">
      <%= link_to "ユーザー一覧", passages_path, class: "list" %>
      <%= link_to "シナリオ一覧", scenarios_path, class: "list" %>
      <li class="list"><a href="#">カレンダー</a></li>
      <li class="list"><a href="#">お知らせ</a></li>
      <li class="list"><a href="#">ユーザー検索</a></li>
    </ul>
  </div>
  <div class="main-box">
    <div class="num">
         ユーザー一覧
    </div>
    <div class="user_box">
      <div class="user_name">
        <div class="calm">
          ユーザー名
        </div>
        <div class="name_box">
          <% @users.each do |user| %>
          <%= link_to user.nickname, passage_path(user.id) %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
